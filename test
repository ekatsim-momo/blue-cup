#include<iostream>
#include<algorithm>
#include<cstdio>
#include<cstring>
#include<string>
#include<vector>
using namespace std;
bool arr[100][100];
int n, start, sumtime=0, sumnum=0,c=0;
void dfs(int time, int v,int last,long double per)
{
	if (time&&v == start||time>250)
	{
		sumtime += time;
		sumnum++;
		return;
	}
	for(int ;;)
	for (int j = 1; j <= n; j++)
	{
		if (arr[v][j] && j != last)
			dfs(time + 1, j, v);
	}
}
int main()
{
	memset(arr, false, sizeof(arr));
	int m,u,v;
	cin >> n >> m;
	while (m--)
	{
		cin >> u >> v;
		arr[u][v] = true;
		arr[v][u] = true;
	}
	for (int i = 3; i <= n; i++)
	{
		sumtime = 0;
		sumnum = 0;
		start = i;
		dfs(0,i,i);
		printf("%.10lf", (long double)sumtime*1.0 / sumnum);
	}
	return 0;
}
